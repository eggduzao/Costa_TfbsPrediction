    keywords = {chip-seq, encode, genome\_bioinformatics, human},
    month = aug,
    pages = {120816073841007+},
    pmid = {22897152},
    posted-at = {2012-08-18 09:31:58},
    priority = {2},
    title = {{Integration of 198 ChIP-seq Datasets Reveals Human cis-Regulatory Regions}},
    year = {2012}
}

@article{buck2004,
    abstract = {{Chromatin immunoprecipitation (ChIP) is a well-established procedure used to investigate interactions between proteins and DNA. Coupled with whole-genome DNA microarrays, ChIPs allow one to determine the entire spectrum of in vivo DNA binding sites for any given protein. The design and analysis of ChIP-microarray (also called ChIP-chip) experiments differ significantly from the conventions used for more traditional microarray experiments that measure relative transcript levels. Furthermore, fundamental differences exist between single-locus ChIP approaches and ChIP-chip experiments, and these differences require new methods of analysis. In this light, we review the design of DNA microarrays, the selection of controls, the level of repetition required, and other critical parameters for success in the design and analysis of ChIP-chip experiments, especially those conducted in the context of mammalian or other relatively large genomes.}},
    author = {Buck, Michael J. and Lieb, Jason D.},
    citeulike-article-id = {1676231},
